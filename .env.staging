# Staging Environment Variables

# App Environment
APP_ENV=staging

# Firebase Configuration (Staging)
FIREBASE_API_KEY=your_staging_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_staging_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_staging_project_id
FIREBASE_STORAGE_BUCKET=your_staging_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_staging_sender_id
FIREBASE_APP_ID=your_staging_app_id

# API Configuration
API_BASE_URL=https://staging-api.mijanul.com/api

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
